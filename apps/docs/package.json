{
  "name": "docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3001",
    "build": "npm run generate:all && next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "generate:all": "npm-run-all --parallel gen:cli",
    "gen:cli": "npm-run-all gen:cli:commands gen:cli:config",
    "gen:cli:commands": "tsx ./generator/index.ts --type cli --input ./spec/cli_v1_commands--old.yaml --output  ./docs/reference/cli/generated/usage.mdx ",
    "gen:cli:config": "tsx ./generator/index.ts --type config --input ./spec/cli_v1_config.yaml --output  ./docs/reference/cli/generated/config.mdx"
  },
  "dependencies": {
    "@code-hike/mdx": "^0.9.0",
    "@mdx-js/loader": "^3.0.0",
    "@mdx-js/react": "^3.0.0",
    "@next/bundle-analyzer": "^14.0.2",
    "@next/mdx": "^14.0.2",
    "@supabase/supabase-js": "^2.38.2",
    "common": "*",
    "config": "*",
    "copy-webpack-plugin": "^11.0.0",
    "gray-matter": "^4.0.3",
    "json-stringify-safe": "^5.0.1",
    "lodash": "^4.17.21",
    "next": "^14.0.2",
    "next-mdx-remote": "^4.4.1",
    "next-plugin-yaml": "^1.0.1",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intersection-observer": "^9.5.3",
    "react-markdown": "^9.0.0",
    "remark": "^15.0.1",
    "ui": "*",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@types/react": "^18.2.24",
    "config": "*",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.8",
    "globby": "^14.0.0",
    "next-transpile-modules": "^10.0.1",
    "npm-run-all": "^4.1.5",
    "openapi-types": "^12.1.3",
    "sass": "^1.69.5",
    "tsconfig": "*",
    "tsx": "^4.1.1",
    "typescript": "^5.2.2"
  }
}
